{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {
        "DEFAULT_MINIMUM_TOKENS": ""
      }
    },
    "sequentialthinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "devtools": {
      "url": "http://127.0.0.1:1337/sse",
      "alwaysAllow": [
        "git_status",
        "git_stage_and_commit",
        "ai_edit",
        "execute_command"
      ]
    }
  },
  "coreTools": [
    "read_many_files",
    "web_fetch"
  ]
}